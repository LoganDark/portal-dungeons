{
	"schemaVersion": 1,
	"id": "portal-dungeons",
	"version": "${version}",
	"name": "Portal Dungeons",
	"description": "High-risk, high-reward dungeons that act like non-euclidean, overlapping labyrinths",
	"authors": [
		"LoganDark"
	],
	"contact": {
		"sources": "https://github.com/LoganDark/portal-dungeons"
	},
	"license": "UNLICENSED",
	"icon": "assets/portal-dungeons/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"adapter": "kotlin",
				"value": "net.logandark.portaldungeons.PortalDungeons"
			}
		],
		"modmenu": [
			{
				"adapter": "kotlin",
				"value": "net.logandark.portaldungeons.ModMenuConfig"
			}
		]
	},
	"mixins": [
		"portal-dungeons.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.7.1",
		"fabric": "*",
		"fabric-language-kotlin": "*",
		"minecraft": "1.15.x"
	},
	"suggests": {
		"flamingo": "*"
	}
